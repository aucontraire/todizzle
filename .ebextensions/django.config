option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "todizzle.settings"
    "PYTHONPATH": "/opt/python/current/app/todizzle:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: todizzle/todizzle/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"
